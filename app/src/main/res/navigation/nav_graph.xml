<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/addSessionView2">
    <fragment
        android:id="@+id/addSessionView2"
        android:name="com.example.pawalk.AddSessionFragment"
        android:label="fragment_add_session_view"
        tools:layout="@layout/fragment_add_session_view" >
        <action
            android:id="@+id/action_addSessionView2_to_sessionScreen"
            app:destination="@id/sessionScreen" />
    </fragment>
    <fragment
        android:id="@+id/mapView2"
        android:name="com.example.pawalk.MapViewFragment"
        android:label="fragment_map_view"
        tools:layout="@layout/fragment_map_view" >
        <action
            android:id="@+id/action_mapView2_to_sessionScreen"
            app:destination="@id/sessionScreen" />
    </fragment>
    <activity
        android:id="@+id/sessionScreen"
        android:name="com.example.pawalk.SessionScreen"
        android:label="activity_session_screen"
        tools:layout="@layout/activity_session_screen" />
    <fragment
        android:id="@+id/searchView2"
        android:name="com.example.pawalk.SearchFragment"
        android:label="fragment_search_view"
        tools:layout="@layout/fragment_search_view" >
        <action
            android:id="@+id/action_searchView2_to_sessionScreen"
            app:destination="@id/sessionScreen" />
    </fragment>
    <fragment
        android:id="@+id/profileView3"
        android:name="com.example.pawalk.ProfileFragment"
        android:label="fragment_profile_view"
        tools:layout="@layout/fragment_profile_view" >
        <action
            android:id="@+id/action_profileView3_to_sessionScreen"
            app:destination="@id/sessionScreen" />
    </fragment>
    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.pawalk.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.pawalk.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" >
        <action
            android:id="@+id/action_secondFragment_to_firstFragment"
            app:destination="@id/firstFragment" />
    </fragment>
</navigation>